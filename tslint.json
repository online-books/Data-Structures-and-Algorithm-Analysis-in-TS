{
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ],
    "include": [
      "src/**/*.ts"
    ]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-bitwise": false,
    "ban-types": false,
    "prefer-for-of": false,
    "no-empty": false
  }
}