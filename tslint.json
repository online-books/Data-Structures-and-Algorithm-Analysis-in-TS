{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/**/*.js",
      "src/lib/OrbitControls.ts"
    ]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-bitwise": false,
    "ban-types": false,
    "prefer-for-of": false
  },
  "jsRules": {}
}