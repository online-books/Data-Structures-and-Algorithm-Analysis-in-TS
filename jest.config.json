{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/SourceCode/$1"
  },
  "collectCoverageFrom": [
    "**/*.ts",
    "!**/node_modules/**"
  ],
  "moduleFileExtensions": [
    "js",
    "ts"
  ],
  "globals": {
    "ts-jest": {}
  },
  "roots": [
    "Tests"
  ]
}