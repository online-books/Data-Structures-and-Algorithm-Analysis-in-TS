 {
     "parser": "@typescript-eslint/parser",
     "plugins": ["@typescript-eslint"],
     "env": {
         "browser": false,
         "node": true
     },
     "settings": {
         "import/resolver": {
             "node": {
                 "extensions": [".ts"]
             },
             "typescript": {
                 "alwaysTryTypes": true
             }
         }
     },
 }